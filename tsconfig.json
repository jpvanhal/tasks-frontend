{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "dom"
    ],
    "paths":{
      "@orbit/coordinator": ["../node_modules/@orbit/coordinator/dist/commonjs/es5"],
      "@orbit/core": ["../node_modules/@orbit/core/dist/commonjs/es5"],
      "@orbit/data": ["../node_modules/@orbit/data/dist/commonjs/es5"],
      "@orbit/indexeddb": ["../node_modules/@orbit/indexeddb/dist/commonjs/es5"],
      "@orbit/store": ["../node_modules/@orbit/store/dist/commonjs/es5"],
      "@orbit/utils": ["../node_modules/@orbit/utils/dist/commonjs/es5"]
    }
  }
}
